%function[image] = infile_read(file_name)
clear all; close all;
file_name = 'learn_and_teach.in';
EOL = 10;
LO = 46;
HI = 35;
fileID = fopen(file_name,'r');
row_col = importdata(file_name);
num_rows = row_col(1);
num_cols = row_col(2);
check = num_rows*num_cols;
fgets(fileID);
[delim_raster_image,count] = fread(fileID);
raster_image = delim_raster_image(delim_raster_image~=EOL);
fclose(fileID);
image = reshape(raster_image,num_rows,num_cols);
image(image==
%end

